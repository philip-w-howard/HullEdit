<Application x:Class="HullEdit.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HullEdit"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:SetupData x:Key="MySetupData" panelWidth="96" panelHeight="48" numPanelsHorizontal="3" numPanelsVertical="2" overallScale="3" />
        <local:GCodeParameters x:Key="GCodeSetup"
                PlungeSpeed="5"
                CutSpeed = "30"
                CutterDiameter = "0.25"
                OffsetInside = "false"
                MaterialThickness = "0.25"
                CutthroughDepth = "0.05"
                DepthPerCut = "0.15"
                NumTabs = "4"
                TabDepth = "0.0625"
                TabWidth = "0.375" />

    </Application.Resources>
</Application>
